<div class="list-card">
    <div class="list-card-image-container">
        <p class="list-card-image-container-img-name">
            {{ notification.title }}
        </p>
    </div>
    <div class="list-card-button-container">
        <button type="button" onclick="window.location = '{{notification.link}}'" class="btn btn-outline-info btn-round list-card-button">
            <i class="fas fa-arrow-right"></i>
        </button>
    </div>
</div>