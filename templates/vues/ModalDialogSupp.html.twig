<!-- Modal Dialog -->
<div class="modal fade" id="dialogModalSupp" tabindex="-1" role="dialog" aria-hidden="true" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{% trans %}message.warning{% endtrans %}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p>{% trans %}message.deleteconf{% endtrans %}</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">{% trans %}button.cancel{% endtrans %}</button>
                <button type="button" class="btn btn-danger" onclick="window.location = '/event/{{ event.id }}/delete';">{% trans %}button.delete{% endtrans %}</button>
            </div>
        </div>
    </div>
</div>