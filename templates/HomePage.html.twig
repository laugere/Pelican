				$('[data-toggle="popover"]').popover()
			})
		</script>

		<title>
			Pelican
		</title>

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<div id="mainFrame">
			<div class="main-container">
				{% include 'vues/NavBar.html.twig' %}
